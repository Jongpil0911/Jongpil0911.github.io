<ul class="card-text font-weight-light list-group list-group-flush">
  {% for content in data[1] | sort: 'startDate' | reverse %}
    <li class="list-group-item">
      <div class="row">
        <div class="col-xs-10 cl-sm-10 col-md-10 mt-2 mt-md-0">
          <h6 class="title font-weight-bold ml-1 ml-md-4">
            {{ content.title }}
          </h6>
          <h6 class="ml-1 ml-md-4" style="font-size: 0.95rem">{{ content.awarder }}</h6>
          
          {% if content.dates_list %}             <ul class="items ml-2">               {% for date_item in content.dates_list %}                 <li style="font-size: 0.85rem; color: #777;">
                  {{ date_item }}                 </li>
              {% endfor %}             </ul>           {% endif %}           
          {% if content.summary %}             <h6 class="ml-1 ml-md-4" style="font-size: 0.95rem; font-style: italic">{{ content.summary }}</h6>
          {% endif %}
        </div>
        <div class="col-xs-10 cl-sm-10 col-md-10 mt-2 mt-md-0">
          <h6 class="title font-weight-bold ml-1 ml-md-4">
            {{ content.title }}
          </h6>
          <h6 class="ml-1 ml-md-4" style="font-size: 0.95rem">{{ content.awarder }}</h6>
          
          {% if content.dates_list %}
            <h6 class="ml-1 ml-md-4" style="font-size: 0.85rem; color: #777;">
              {{ date_item }}
              </li>
                {% endfor %}
              </ul>
              {% endif %}
              {% if content.summary %}
            </h6>
          <h6 class="ml-1 ml-md-4" style="font-size: 0.95rem; font-style: italic">{{ content.summary }}</h6>
          {% endif %}
        </div>
      </div>
    </li>
  {% endfor %}
</ul>
